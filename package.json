      "@angular/flex-layout": "14.0.0-beta.41"
    },
    "minimist": ">=1.2.6",
    "xml2js": "0.5.0",
    "postcss": "8.4.31",
    "request": "npm:@cypress/request@^3.0.0",
    "ajv": "8.17.1"
